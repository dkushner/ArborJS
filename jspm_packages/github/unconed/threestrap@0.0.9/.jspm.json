{
  "name": "threestrap",
  "version": "0.0.9",
  "description": "Minimal Three.js Bootstrapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/unconed/threestrap.git"
  },
  "main": "threestrap",
  "scripts": {
    "build": "gulp",
    "test": "gulp test"
  },
  "devDependencies": {
    "gulp": "^3.4.0",
    "gulp-uglify": "^0.1.0",
    "gulp-concat": "^2.1.7",
    "gulp-rename": "^0.2.2",
    "gulp-karma": "^0.0.2",
    "karma": "^0.10.10",
    "lodash": "^2.4.1",
    "run-sequence": "^0.3.6",
    "gulp-watch": "^0.5.2",
    "optimist": "^0.6.1"
  },
  "author": "Steven Wittens",
  "license": "MIT",
  "jspm": {
    "main": "threestrap",
    "format": "global",
    "directories": {
      "dist": "build"
    },
    "registry": "jspm",
    "dependencies": {
      "three": "*"
    },
    "shim": {
      "threestrap": {
        "deps": [
          "three"
        ],
        "exports": "THREE"
      }
    }
  },
  "format": "global",
  "directories": {
    "dist": "build"
  },
  "registry": "jspm",
  "shim": {
    "threestrap": {
      "deps": [
        "three"
      ],
      "exports": "THREE"
    }
  },
  "dependencies": {
    "three": "*"
  }
}